# auto-indent
set autoindent

# cut to end of line with ^K
set cut

# enable history
set historylog

# don't wrap text
set nowrap

# set tabs to 4 spaces
set tabsize 4

# automatically save on exit
set tempfile

# allow sending to background
set suspend

