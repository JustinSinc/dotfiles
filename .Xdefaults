!
! ~/.Xdefaults
!

! urxvt

urxvt.inheritPixmap:     true
urxvt.reverseVideo:      false
urxvt.shading:           0 !trans, 0=opaque
urxvt.fading:            60 !term unfocus
urxvt.font:              xft:Terminus:size=12
urxvt.scrollBar:         false
urxvt.saveLines:         5500
urxvt.termName:          screen-256color
urxvt.internalBorder:    0
urxvt.cursorBlink:       false
urxvt.tintColor:         #dfdfdf
urxvt.cursorColor:       #dd9900
urxvt.cursorColor2:      #000000
urxvt.background:        #000000
urxvt.foreground:        #ddccbb
urxvt.colorBD:           #ffffff
urxvt.colorIT:           #bbbbbb
urxvt.colorUL:           #999999
urxvt.underlineColor:    #999999
URxvt*perl-ext-common:	default,matcher,clipboard

! xterm

xterm*faceName:          Terminus:size=12
xterm*geometry:          127x36
xterm*boldMode:          true
xterm*metaSendsEscape:   true
xterm*background:        #292929
xterm*foreground:        #ddccbb
